<!-- Main templates -->
<message id="GS.Main">
${navigation class="navbar-g"}
${content}
</message>

<message id="GS.NavigationBar">
<div class="${container-class}">
	${collapse-button}
	<div class="navbar-header">
		${expand-button}
		${title-link}
	</div>
	${contents}
</div>
</message>

<message id="GS.Admin.Main">
<div class="container-fluid">
	<div class="row">
		<div class="col-sm-2 no-padding">
			${sidebar class="navbar-admin-side"}
		</div>
		<div class="col-sm-10 col-print-12">
			${content}
		</div>
	</div>
</div>
</message>

<message id="GS.Admin.Main.Content">
<div class="row">
	<h2 style="padding:0 20px;">${title}</h2>
	<hr />
</div>
<div class="container-fluid">
	<div class="row">${content}</div>
</div>
</message>

<!-- HeightEdit -->
<message id="GS.HeightEdit">
<div class="input-group">
	${<is-cm>}${cm-edit}${</is-cm>}
	${<is-ft>}
		${ft-edit}
		<span class="input-group-addon">feet</span>
		${in-edit}
		<span class="input-group-addon">inches</span>
	${</is-ft>}
	<div class="input-group-btn">${unit-select}</div>
</div>
</message>

<!-- FilteredList -->
<message id="GS.ListFilterView">
<div class="input-group">
	<label class="input-group-addon" for="${id:widget}">${title}</label>
	${widget}
	<span class="input-group-addon">${checkbox}</span>
</div>
</message>

<message id="GS.ListRangeFilterView">
<div class="input-group">
	<span class="input-group-addon">${title}</span>
	${operatorCombo}
	<span class="input-group-addon"></span>
	${widget}
	<span class="input-group-addon">${checkbox}</span>
</div>
</message>

<message id="GS.FiltersTemplate">
<div class="panel panel-default panel-filters">
	<div class="panel-heading"><h3 class="panel-title">${tr:Filter}</h3></div>
	<div class="panel-body">
		<div class="row">
			<div class="col-sm-8">${filters-container}</div>
			<div class="col-sm-4">
				<div class="input-group">
					<span class="input-group-addon">${tr:AddFilter}</span>
					${filters-combo}
					<span class="input-group-btn">${add-filter}</span>
				</div>
			</div>
		</div>
		<div class="pull-right">${apply-filter}</div>
	</div>
</div>
</message>

<message id="GS.FilteredListView">
${<filters-enabled>}${filters}${</filters-enabled>}
${table-view}
</message>

<!-- EntityView -->
<message id="GS.Admin.Entities.New">
${<type-selection>}
	<div class="text-center">
		<h2>${tr:SelectEntityType}</h2>
		<div class="btn-group" role="group">
			${selectPerson}
			${selectBusiness}
		</div>
	</div>
${</type-selection>}

${<type-chosen>}
	${<type-selection>}<hr />${</type-selection>}
	
	${<is-person>}
		<div class="panel panel-default">
			<div class="panel-heading">${tr:PersonalInfo}</div>
			<div class="panel-body form-horizontal">
				<div class="row row-entity-upload">
					<div class="col-md-3">${profileUpload class="profile-pic"}</div>
					<div class="row row-entity-upload col-md-9">
						<div class="col-md-6">${cnicUpload class="cnic-pic"}</div>
						<div class="col-md-6">${cnicUpload2 class="cnic-pic"}</div>
					</div>
				</div>
				
				${block:GS.FieldH name FullName}
				${block:GS.FieldH dob DateOfBirth}
				${block:GS.FieldH cnic CNICLabel}
				${block:GS.FieldH identificationMark IdentificationMark}
				${block:GS.FieldH.CW height Height}
				${block:GS.FieldH bloodType BloodType}
				${block:GS.FieldH motherTongue MotherTongue}
				${block:GS.FieldH married MaritalStatus}
				${block:GS.FieldH.CW nextOfKin NextOfKin}
				${block:GS.FieldH.CW father Father}
				${block:GS.FieldH.CW mother Mother}
				${block:GS.FieldH remarks Remarks}
			</div>
		</div>
		
		${block:GS.Admin.Entities.ContactBlock}
		
		${<m:employee>}
			<div class="panel panel-default">
				<div class="panel-heading">${tr:EmployeeInfo}</div>
				<div class="panel-body form-horizontal">
					${block:GS.FieldH companyNumber CompanyNumber}
					${block:GS.FieldH recruitmentDate RecruitmentDate}
					${block:GS.FieldH education Education}
					${block:GS.FieldH experience Experience}
					${block:GS.FieldH addQualifications GradeLabel}
					${block:GS.FieldH grade AdditionalQualifications}
				</div>
			</div>
		${</m:employee>}
		
		${<m:personnel>}
			<div class="panel panel-default">
				<div class="panel-heading">${tr:PersonnelInfo}</div>
				<div class="panel-body form-horizontal">
						${block:GS.FieldH policeStation PoliceStation}
						${block:GS.FieldH.CB policeVerified PoliceVerified}
						${block:GS.FieldH armyNumber ArmyNumber}
						${block:GS.FieldH rank Rank}
						${block:GS.FieldH trainingCourses TrainingCourses}
				</div>
			</div>
		${</m:personnel>}
	${</is-person>}

	${<is-business>}
	<div class="panel panel-default">
		<div class="panel-heading">${tr:BusinessInfo}</div>
		<div class="panel-body form-horizontal">
			${block:GS.FieldH name BusinessName}
		</div>
	</div>
	${block:GS.Admin.Entities.ContactBlock}
	${</is-business>}
	
	${block:GS.Admin.Entities.RecurringIncomesBlock}
	${block:GS.Admin.Entities.RecurringExpensesBlock}
	
	${submitBtn class="btn-primary"}
	${<is-person>}
		${add-employee}
		${add-personnel}
	${</is-person>}
${</type-chosen>}
</message>

<message id="GS.Admin.Entities.ClientBlock">
<div class="panel panel-default">
	<div class="panel-heading">${tr:ClientInfo}</div>
	<div class="panel-body">
		NYI
	</div>
</div>
